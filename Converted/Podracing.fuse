function Process(req)

  local imgattrs = {
    IMG_Document = self.Comp,
    { IMG_Channel = "Red", },
    { IMG_Channel = "Green", },
    { IMG_Channel = "Blue", },
    { IMG_Channel = "Alpha", },
    IMG_Width  = Width,
    IMG_Height = Height,
    IMG_XScale = XAspect,
    IMG_YScale = YAspect,
    IMAT_OriginalWidth  = realwidth, -- nil !?!
    IMAT_OriginalHeight = realheight, -- nil !?!
    IMG_Quality = not req:IsQuick(),
    IMG_MotionBlurQuality = not req:IsNoMotionBlur(),
    IMG_DeferAlloc = true,
    IMG_ProxyScale = ( (not req:IsStampOnly()) and 1 or nil),
    IMG_Depth = ( (SourceDepth~=0) and SourceDepth or nil   )
  }

  local dst   = Image(imgattrs)
  local black = Pixel({R=0,G=0,B=0,A=0})
  dst:Fill(black)

  if req:IsPreCalc() then
    local out = Image({IMG_Like = dst, IMG_NoData = true})
    OutImage:Set(req, out)
    return
  end

  local node = DVIPComputeNode(req,
    "PodracingFuse", ShaderCompatibilityCode..ShaderKernelCode,
    "Params", ShaderParameters
  )

  -- Extern texture or create a new one

  iChannel0 = InChannel0:GetValue(req)

  if iChannel0==nil then
    iChannel0 = Image(imgattrs)
    iChannel0:Fill(black)
  end

  iChannel1 = InChannel1:GetValue(req)

  if iChannel1==nil then
    iChannel1 = Image(imgattrs)
    iChannel1:Fill(black)
  end

  iChannel2 = InChannel2:GetValue(req)

  if iChannel2==nil then
    iChannel2 = Image(imgattrs)
    iChannel2:Fill(black)
  end

  iChannel3 = InChannel3:GetValue(req)

  if iChannel3==nil then
    iChannel3 = Image(imgattrs)
    iChannel3:Fill(black)
  end

  iChannel4 = InChannel4:GetValue(req)

  if iChannel4==nil then
    iChannel4 = Image(imgattrs)
    iChannel4:Fill(black)
  end

  -- DCTL parameters

  local framerate = self.Comp:GetPrefs("Comp.FrameFormat.Rate")

  local params = {}

  params = node:GetParamBlock(ShaderParameters)

  params.iResolution[0] = dst.Width
  params.iResolution[1] = dst.Height
  params.iTime = (req.Time / framerate) * InFrequency:GetValue(req).Value
  
  -- iMouse
  
  local mouse_xy  = InMouseXY:GetValue(req)
  local mouse_zw  = InMouseZW:GetValue(req)
  
  params.iMouse[0] = mouse_xy.X
  params.iMouse[1] = mouse_xy.Y
  params.iMouse[2] = mouse_zw.X
  params.iMouse[3] = mouse_zw.Y
  
  if InMouseDrag:GetValue(req).Value ~= 0 then
    if params.iMouse[2]==-1 and params.iMouse[3]==-1 then
      params.iMouse[2]=params.iMouse[0]
      params.iMouse[3]=params.iMouse[1]
    end
  else
    params.iMouse[2] = -1
    params.iMouse[3] = -1
  end
  
  if mouse_zw.X ~= params.iMouse[2] or mouse_zw.Y ~= params.iMouse[3] then
    InMouseZW:SetAttrs({INP_Disabled=false})
    InMouseZW:SetSource(Point(params.iMouse[2],params.iMouse[3]),0,0)
    InMouseZW:SetAttrs({INP_Disabled=true})
  end
  
  params.iMouse[0] = params.iMouse[0] * Width
  params.iMouse[1] = params.iMouse[1] * Height
  if params.iMouse[2] == -1 and params.iMouse[3] == -1 then
    params.iMouse[2] = 0
    params.iMouse[3] = 0
  else
    params.iMouse[2] = params.iMouse[2] * Width
    params.iMouse[3] = params.iMouse[3] * Height
  end

  params.Tex = InTexCheckbox:GetValue(req).Value
  params.Tracking = InTrackingCheckbox:GetValue(req).Value
  params.PlaneTex = InPlaneTexCheckbox:GetValue(req).Value
  params.AA = InAASlider:GetValue(req).Value
  params.ColorSky = {
    InColorSkyColorR:GetValue(req).Value,
    InColorSkyColorG:GetValue(req).Value,
    InColorSkyColorB:GetValue(req).Value,InColorSkyColorA:GetValue(req).Value
  }
  params.ColorReactor = {
    InColorReactorColorR:GetValue(req).Value,
    InColorReactorColorG:GetValue(req).Value,
    InColorReactorColorB:GetValue(req).Value,InColorReactorColorA:GetValue(req).Value
  }
  params.ColorCockpit = {
    InColorCockpitColorR:GetValue(req).Value,
    InColorCockpitColorG:GetValue(req).Value,
    InColorCockpitColorB:GetValue(req).Value,InColorCockpitColorA:GetValue(req).Value
  }
  params.ColorGround = {
    InColorGroundColorR:GetValue(req).Value,
    InColorGroundColorG:GetValue(req).Value,
    InColorGroundColorB:GetValue(req).Value,InColorGroundColorA:GetValue(req).Value
  }
  params.Color5 = {
    InColor5ColorR:GetValue(req).Value,
    InColor5ColorG:GetValue(req).Value,
    InColor5ColorB:GetValue(req).Value,InColor5ColorA:GetValue(req).Value
  }
  params.ColorFog = {
    InColorFogColorR:GetValue(req).Value,
    InColorFogColorG:GetValue(req).Value,
    InColorFogColorB:GetValue(req).Value,InColorFogColorA:GetValue(req).Value
  }
  params.ViewZ = InViewZSlider:GetValue(req).Value
  params.ViewXY = {InViewXYPoint:GetValue(req).X,InViewXYPoint:GetValue(req).Y}
  params.CamZ = InCamZSlider:GetValue(req).Value
  params.CamXY = {InCamXYPoint:GetValue(req).X,InCamXYPoint:GetValue(req).Y}
  params.amplitudeRz = InamplitudeRzSlider:GetValue(req).Value
  params.amplitudeRy = InamplitudeRySlider:GetValue(req).Value
  params.amplitudeRx = InamplitudeRxSlider:GetValue(req).Value
  params.frequency = InfrequencySlider:GetValue(req).Value
  params.lenReactor = InlenReactorSlider:GetValue(req).Value
  params.radius = InradiusSlider:GetValue(req).Value
  params.Tex1 = InTex1Slider:GetValue(req).Value
  params.Tex2 = InTex2Slider:GetValue(req).Value
  params.Tex3 = InTex3Slider:GetValue(req).Value
  params.ShipPosZ = InShipPosZSlider:GetValue(req).Value
  params.ShipPosXY = {InShipPosXYPoint:GetValue(req).X,InShipPosXYPoint:GetValue(req).Y}
  params.Clouds = InCloudsSlider:GetValue(req).Value
  params.PathPar1 = InPathPar1Slider:GetValue(req).Value
  params.PathPar2 = InPathPar2Slider:GetValue(req).Value
  params.PathPar3 = InPathPar3Slider:GetValue(req).Value
  params.PathPar4 = InPathPar4Slider:GetValue(req).Value
  params.PathPar5 = InPathPar5Slider:GetValue(req).Value
  params.MAX_STEPS = InMAX_STEPSSlider:GetValue(req).Value
  params.TimeModulo = InTimeModuloSlider:GetValue(req).Value
  params.Fog = InFogSlider:GetValue(req).Value
  params.TexUL = {InTexULPoint:GetValue(req).X,InTexULPoint:GetValue(req).Y}
  params.TexOR = {InTexORPoint:GetValue(req).X,InTexORPoint:GetValue(req).Y}
  params.TexOffset = {InTexOffsetPoint:GetValue(req).X,InTexOffsetPoint:GetValue(req).Y}
  params.romul = InromulSlider:GetValue(req).Value
  -- Resolution

  params.width  = dst.Width
  params.height = dst.Height

  -- Per channel time and resolution


  -- Set parameters and add I/O

  node:SetParamBlock(params)
  node:AddSampler("RowSampler", TEX_FILTER_MODE_LINEAR,TEX_ADDRESS_MODE_MIRROR, TEX_NORMALIZED_COORDS_TRUE)
  node:AddInput("iChannel0",iChannel0) -- TODO: add a better channel name
  node:AddInput("iChannel1",iChannel1) -- TODO: add a better channel name
  node:AddInput("iChannel2",iChannel2) -- TODO: add a better channel name
  node:AddInput("iChannel3",iChannel3) -- TODO: add a better channel name
  node:AddInput("iChannel4",iChannel4) -- TODO: add a better channel name
  node:AddOutput("dst", dst)

  local ok = node:RunSession(req)

	if (not ok) then
		dst = nil
    dump(node:GetErrorLog())
	end

  OutImage:Set(req,dst)
end



